<p>List of Student</p>


<table border="1">
  <tr>
    <td>ID</td>
    <td>Name</td>
    <td>Age</td>
    <td>Gender</td>
  </tr>
<% @students.each do |student| %>
  <tr>
      <td><%= student.id %></td>
      <td><%= student.name %></td>
      <td><%= student.age %></td>
      <td><%= student.gender %></td>
  </tr>
<% end %>
</table>
